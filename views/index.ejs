


<!DOCTYPE html>
<html lang="en">
<head>
    <%- include ('./partials/head.ejs') %>
</head>
<body>
    <div class='container'>
    <h1>Home Page</h1>
    <ul>
        <% for(let i = 0; i < allPokemon.length; i++) { %>
            <li>
               <a href="/pokemon/<%= i %>"> 
                <img src="<%= allPokemon[i].img%>" alt="pokemon image">
            </a>
            </li>
            <!-- DELETE ADD HERE -->
            <form action="/pokemon/<%= i %>?_method=DELETE" method="POST">
                <input type="submit" value="DELETE"/>
            </form>
            <button><a href="/pokemon/<%= i %>/edit">EDIT</a></button>
          
            <% } %>
    </ul>
    <button><a href="/pokemon/new">Add New Scientist</a></button>
</div>
<% console.log(allPokemon[allPokemon.length - 1]) %>
</body>
</html>